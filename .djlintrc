{
    "profile": "django",
    "indent": 4,
    "max_line_length": 120,
    "format_css": true,
    "format_js": true,
    "ignore": "H006,H030,H031,T002,T003,D018",
    "exclude": ".venv,venv,env,node_modules,migrations",
    "blank_line_after_tag": "load,extends,include",
    "blank_line_before_tag": "block,endblock",
    "close_void_tags": false,
    "custom_blocks": "",
    "custom_html": "",
    "format_attribute_template_tags": true,
    "ignore_blocks": "",
    "ignore_case": true,
    "linter_output_format": "{code}({line}): {message}",
    "max_attribute_length": 70,
    "max_blank_lines": 1,
    "preserve_blank_lines": false,
    "preserve_leading_space": false,
    "require_pragma": false,
    "use_gitignore": true
}
